<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="./style.xml" ?>
<definition>
  <title>Double application</title>
  <ident>double-app</ident>
  <value>
    <function>
      <bindings>
        <binding name="f"/>
        <binding name="x"/>
      </bindings>
      <body>
        <application>
          <head>
            <variable name="f">1</variable>
          </head>
          <argument>
            <variable name="x">0</variable>
          </argument>
          <argument>
            <variable name="x">0</variable>
          </argument>
        </application>
      </body>
    </function>
  </value>
</definition>