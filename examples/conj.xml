<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="./style.xml" ?>
<definition>
  <title>Logical conjunction</title>
  <ident>conj</ident>
  <value>
    <function>
      <bindings>
        <binding name="P"/>
        <binding name="Q"/>
      </bindings>
      <body>
        <application>
          <head>
            <reference>forall</reference>
          </head>
          <argument>
            <function>
              <bindings>
                <binding name="R"/>
              </bindings>
              <body>
                <application>
                  <head>
                    <reference>implies</reference>
                  </head>
                  <argument>
                    <application>
                      <head>
                        <reference>implies</reference>
                      </head>
                      <argument>
                        <variable name="P">2</variable>
                      </argument>
                      <argument>
                        <application>
                          <head>
                            <reference>implies</reference>
                          </head>
                          <argument>
                            <variable name="Q">1</variable>
                          </argument>
                          <argument>
                            <variable name="R">0</variable>
                          </argument>
                        </application>
                      </argument>
                    </application>
                  </argument>
                  <argument>
                    <variable name="R">0</variable>
                  </argument>
                </application>
              </body>
            </function>
          </argument>
        </application>
      </body>
    </function>
  </value>
</definition>