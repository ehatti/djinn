<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="./style.xml" ?>
<theorem>
  <title>Reflexivity of equality</title>
  <ident>eq-refl</ident>
  <goal>
    <application>
      <head>
        <reference>forall</reference>
      </head>
      <argument>
        <function>
          <bindings>
            <binding name="x"/>
          </bindings>
          <body>
            <application>
              <head>
                <reference>eq</reference>
              </head>
              <argument>
                <variable name="x">0</variable>
              </argument>
              <argument>
                <variable name="x">0</variable>
              </argument>
            </application>
          </body>
        </function>
      </argument>
    </application>
  </goal>
  <proof>
    <introduce>
      <suppose>
        <binding name="x"/>
      </suppose>
      <goal>
        <application>
          <head>
            <reference>eq</reference>
          </head>
          <argument>
            <variable name="x">0</variable>
          </argument>
          <argument>
            <variable name="x">0</variable>
          </argument>
        </application>
      </goal>
      <proof>
        <apply>
          <cite>eq</cite>
          <specialize name="P">
            <function>
              <bindings>
                <binding name="F"/>
              </bindings>
              <body>
                <application>
                  <head>
                    <variable name="F">0</variable>
                  </head>
                  <argument>
                    <variable name="x">1</variable>
                  </argument>
                  <argument>
                    <variable name="x">1</variable>
                  </argument>
                </application>
              </body>
            </function>
          </specialize>
          <justify>
            <goal>
              <application>
                <head>
                  <reference>forall</reference>
                </head>
                <argument>
                  <function>
                    <bindings>
                      <binding name="P"/>
                    </bindings>
                    <body>
                      <application>
                        <head>
                          <reference>implies</reference>
                        </head>
                        <argument>
                          <application>
                            <head>
                              <variable name="P">0</variable>
                            </head>
                            <argument>
                              <variable name="x">1</variable>
                            </argument>
                          </application>
                        </argument>
                        <argument>
                          <application>
                            <head>
                              <variable name="P">0</variable>
                            </head>
                            <argument>
                              <variable name="x">1</variable>
                            </argument>
                          </application>
                        </argument>
                      </application>
                    </body>
                  </function>
                </argument>
              </application>
            </goal>
            <proof>
              <introduce>
                <suppose>
                  <binding name="P"/>
                </suppose>
                <goal>
                  <application>
                    <head>
                      <reference>implies</reference>
                    </head>
                    <argument>
                      <application>
                        <head>
                          <variable name="P">0</variable>
                        </head>
                        <argument>
                          <variable name="x">1</variable>
                        </argument>
                      </application>
                    </argument>
                    <argument>
                      <application>
                        <head>
                          <variable name="P">0</variable>
                        </head>
                        <argument>
                          <variable name="x">1</variable>
                        </argument>
                      </application>
                    </argument>
                  </application>
                </goal>
                <proof>
                  <apply>
                    <cite>im-triv</cite>
                    <specialize name="P">
                      <application>
                        <head>
                          <variable name="P">0</variable>
                        </head>
                        <argument>
                          <variable name="x">1</variable>
                        </argument>
                      </application>
                    </specialize>
                  </apply>
                </proof>
              </introduce>
            </proof>
          </justify>
        </apply>
      </proof>
    </introduce>
  </proof>
</theorem>