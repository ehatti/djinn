<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="./style.xml" ?>
<theorem>
  <title>Conjunction introduction</title>
  <ident>conj-intro</ident>
  <goal>
    <application>
      <head>
        <reference>forall</reference>
      </head>
      <argument>
        <function>
          <bindings>
            <binding name="P"/>
          </bindings>
          <body>
            <application>
              <head>
                <reference>forall</reference>
              </head>
              <argument>
                <function>
                  <bindings>
                    <binding name="Q"/>
                  </bindings>
                  <body>
                    <application>
                      <head>
                        <reference>implies</reference>
                      </head>
                      <argument>
                        <variable name="P">1</variable>
                      </argument>
                      <argument>
                        <application>
                          <head>
                            <reference>implies</reference>
                          </head>
                          <argument>
                            <variable name="Q">0</variable>
                          </argument>
                          <argument>
                            <application>
                              <head>
                                <reference>conj</reference>
                              </head>
                              <argument>
                                <variable name="P">1</variable>
                              </argument>
                              <argument>
                                <variable name="Q">0</variable>
                              </argument>
                            </application>
                          </argument>
                        </application>
                      </argument>
                    </application>
                  </body>
                </function>
              </argument>
            </application>
          </body>
        </function>
      </argument>
    </application>
  </goal>
  <proof>
    <introduce>
      <suppose>
        <binding name="P"/>
        <binding name="Q"/>
        <hypothesis name="hP">
          <variable name="P">1</variable>
        </hypothesis>
        <hypothesis name="hQ">
          <variable name="Q">0</variable>
        </hypothesis>
      </suppose>
      <goal>
        <application>
          <head>
            <reference>conj</reference>
          </head>
          <argument>
            <variable name="P">1</variable>
          </argument>
          <argument>
            <variable name="Q">0</variable>
          </argument>
        </application>
      </goal>
      <proof>
        <apply>
          <cite>conj</cite>
          <specialize name="P">
            <function>
              <bindings>
                <binding name="F"/>
              </bindings>
              <body>
                <application>
                  <head>
                    <variable name="F">0</variable>
                  </head>
                  <argument>
                    <variable name="P">2</variable>
                  </argument>
                  <argument>
                    <variable name="Q">1</variable>
                  </argument>
                </application>
              </body>
            </function>
          </specialize>
          <justify>
            <goal>
              <application>
                <head>
                  <reference>forall</reference>
                </head>
                <argument>
                  <function>
                    <bindings>
                      <binding name="R"/>
                    </bindings>
                    <body>
                      <application>
                        <head>
                          <reference>implies</reference>
                        </head>
                        <argument>
                          <application>
                            <head>
                              <reference>implies</reference>
                            </head>
                            <argument>
                              <variable name="P">2</variable>
                            </argument>
                            <argument>
                              <application>
                                <head>
                                  <reference>implies</reference>
                                </head>
                                <argument>
                                  <variable name="Q">1</variable>
                                </argument>
                                <argument>
                                  <variable name="R">0</variable>
                                </argument>
                              </application>
                            </argument>
                          </application>
                        </argument>
                        <argument>
                          <variable name="R">0</variable>
                        </argument>
                      </application>
                    </body>
                  </function>
                </argument>
              </application>
            </goal>
            <proof>
              <introduce>
                <suppose>
                  <binding name="R"/>
                  <hypothesis name="hI">
                    <application>
                      <head>
                        <reference>implies</reference>
                      </head>
                      <argument>
                        <variable name="P">2</variable>
                      </argument>
                      <argument>
                        <application>
                          <head>
                            <reference>implies</reference>
                          </head>
                          <argument>
                            <variable name="Q">1</variable>
                          </argument>
                          <argument>
                            <variable name="R">0</variable>
                          </argument>
                        </application>
                      </argument>
                    </application>
                  </hypothesis>
                </suppose>
                <goal>
                  <variable name="R">0</variable>
                </goal>
                <proof>
                  <apply>
                    <assume name="hI">0</assume>
                    <justify>
                      <goal>
                        <variable name="P">2</variable>
                      </goal>
                      <proof>
                        <apply>
                          <assume name="hP">2</assume>
                          <error>
                            Result of application does not match goal
                            <variable name="Q">1</variable>
                            <variable name="P">2</variable>
                          </error>
                        </apply>
                      </proof>
                    </justify>
                    <justify>
                      <goal>
                        <variable name="Q">1</variable>
                      </goal>
                      <proof>
                        <apply>
                          <assume name="hQ">1</assume>
                          <error>
                            Result of application does not match goal
                            <variable name="R">0</variable>
                            <variable name="Q">1</variable>
                          </error>
                        </apply>
                      </proof>
                    </justify>
                  </apply>
                </proof>
              </introduce>
            </proof>
          </justify>
        </apply>
      </proof>
    </introduce>
  </proof>
</theorem>