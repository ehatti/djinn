<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="./style.xml" ?>
<definition>
  <title>Equality</title>
  <ident>eq</ident>
  <value>
    <function>
      <bindings>
        <binding name="x"/>
        <binding name="y"/>
      </bindings>
      <body>
        <application>
          <head>
            <reference>forall</reference>
          </head>
          <argument>
            <function>
              <bindings>
                <binding name="P"/>
              </bindings>
              <body>
                <application>
                  <head>
                    <reference>implies</reference>
                  </head>
                  <argument>
                    <application>
                      <head>
                        <variable name="P">0</variable>
                      </head>
                      <argument>
                        <variable name="x">2</variable>
                      </argument>
                    </application>
                  </argument>
                  <argument>
                    <application>
                      <head>
                        <variable name="P">0</variable>
                      </head>
                      <argument>
                        <variable name="y">1</variable>
                      </argument>
                    </application>
                  </argument>
                </application>
              </body>
            </function>
          </argument>
        </application>
      </body>
    </function>
  </value>
</definition>